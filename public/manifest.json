{
  "name": "CurateAi",
  "description": "CurateAi Extension",
  "version": "1.1.7",
  "manifest_version": 3,
  "icons": {
    "16": "./f-logo.png",
    "48": "./f-logo.png",
    "128": "./f-logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "f-logo.png",
    "default_title": "CurateAi",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "notifications",
    "alarms",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [],
      "css": []
    }
  ]
}